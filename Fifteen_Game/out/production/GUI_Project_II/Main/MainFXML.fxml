<Pane prefHeight="640.0" prefWidth="480.0" styleClass="background" stylesheets="@../Resources/GameStyle.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.Main">
    <children>
        <Button layoutX="150.0" layoutY="230.0" mnemonicParsing="false" onAction="#newGameButton" prefHeight="47.0" prefWidth="300.0" text="NEW GAME">
         <effect>
            <DropShadow color="WHITE" />
         </effect></Button>
        <Button layoutX="150.0" layoutY="313.0" mnemonicParsing="false" onAction="#scoresButton" prefHeight="47.0" prefWidth="300.0" text="SCORES">
         <effect>
            <DropShadow color="WHITE" />
         </effect></Button>
        <Button layoutX="150.0" layoutY="393.0" mnemonicParsing="false" onAction="#exitButton" prefHeight="47.0" prefWidth="300.0" text="EXIT">
         <effect>
            <DropShadow color="WHITE" />
         </effect></Button>
        <Label layoutX="80.0" layoutY="113.0" prefHeight="62.0" prefWidth="364.0" text="   PUZZLEGAME" />
    </children>
